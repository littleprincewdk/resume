    "online"
  ],
  "author": "littleprincewdk <2889110370@qq.com>",
  "repository": "https://github.com/littleprincewdk/resume",
  "homepage": "https://github.com/littleprincewdk/resume",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack serve --open",
    "build": "cross-env NODE_ENV=production webpack --config webpack-dist.config.js",
    "lint:script": "eslint \"**/*.js\" --report-unused-disable-directives",
    "lint-fix:script": "npm run lint:script -- --fix || exit 0",
    "lint-analyze:script": "mkdir -p logs && npm run lint:script -- --no-color > logs/eslint-analyze.log || exit 0",
    "lint:style": "stylelint --report-needless-disables './**/*.less'",
    "lint-fix:style": "npm run lint:style -- --fix  || exit 0",
    "lint-analyze:style": "npm run lint:style -- --no-color -o logs/stylelint-analyze.log  || exit 0",
    "lint-fix:template": "prettier --write ./src/**/*.html"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-angular": "^12.1.4",
    "autoprefixer": "^9.8.0",
    "chrome-finder": "^1.0.7",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "cross-env": "^7.0.3",
    "css-loader": "^3.5.3",
    "end-webpack-plugin": "^4.0.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.2",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^6.0.0",
    "less": "^4.1.1",
    "less-loader": "^9.0.0",
    "lint-staged": "^11.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "normalize.css": "^8.0.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.3.0",
    "style-loader": "^1.2.1",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.2.0",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2"
  }
}
